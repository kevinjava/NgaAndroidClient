    <string name="md__drawerClosedIndicatorDesc">Open drawer</string>
    <string name="md__drawerOpenIndicatorDesc">Close drawer</string>
    <!-- From: file:/home/kevliu/mip/workspace/ngaclient/menudrawer/res/values/styles.xml -->
